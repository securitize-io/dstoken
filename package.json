{
  "name": "Digital_Securities",
  "version": "v2.4.0",
  "description": "Digital Securities Reference Implementation",
  "scripts": {
    "test": "npm run test:unitests && npm run test:integration",
    "test:unitests": "cross-env TEST_MODE=TRUE ./scripts/test.sh ./test/unitests/**",
    "test:integration": "cross-env TEST_MODE=TRUE ./scripts/test.sh ./test/integration/**",
    "coverage": "truffle compile && ./node_modules/.bin/solidity-coverage",
    "compile": "truffle compile",
    "migrate": "truffle migrate",
    "generate": "node ./generators/EternalStorageClientGenerator.js",
    "remixd": "./node_modules/.bin/remixd --remix-ide https://remix.ethereum.org -s ./"
  },
