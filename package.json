    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eth-gas-reporter": "^0.2.25",
    "ethereumjs-util": "^7.1.5",
    "ganache": "^7.4.0",
    "minimist": "^1.2.7",
